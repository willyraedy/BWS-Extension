{
  "name": "BWS",
  "version": "0.1",
  "description": "Bring Better World Shopping Guide to your browser",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "page": "layout.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": ["bootstrap.css" ],
      "js": ["jquery.js", "bootstrap.js", "cs.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
